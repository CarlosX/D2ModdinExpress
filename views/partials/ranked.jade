#fullHomeBack(style="background:url('http://i.imgur.com/Sp3elDj.jpg') no-repeat")
.container.titleContainer
  .row-fluid
    .span12
      .description-texts
        h1.site-slogan
          span(style="line-height:5px") Ranked Matchmaking
        p.app-description
          .feature(ng-repeat="mod in mods | filter:{ranked:true}")
            span(style="font-size:21px") Reflex MMR: 
              b {{(auth.user.profile.mmr[mod.name] || 2000) | number}}
            br
            a.btn.btn-success(ng-click="startQueue(mod._id)") Queue
            a.btn.btn-info Leaderboards
