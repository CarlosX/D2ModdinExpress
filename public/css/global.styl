body, html
  width: 100%
  height: 100%
  padding: 0

.tileTitle
  text-align center
  font-weight bold
  margin-top 5px
.container
  width:100%
  height:100%
  margin:0
  padding:0
.btn
  border-radius:0
.entryPanel
  width: 400px
  padding-bottom:10px
  margin-left: auto;
  margin-right: auto;
  margin-top: 20px;
.modThumbnail
  width: 250px
  height: 70px
  max-width: 300px
  max-height: 80px
  padding: 10px
  border-color: rgb(230, 230, 230);
  border-width: 1px;
  border-style: solid;
  text-align: left
  cursor: pointer
.modThumbnail .thumbDetail
  width: 170px
  height: 50px;
  float: right
.modThumbnail .thumbDetail p
  font-size: 12px
  margin: 0
  line-height: 15px
.avi
  margin-right: 10px
.avi img
  width 15px
  height 15px
.ilteams p
  margin:0
.installDialog
  width: 30%
  margin: 0 auto
  margin-top: 120px
  box-shadow: 0 0 5px 2px rgba(0, 0, 0, 0.27);
  padding: 20px;
  min-width: 350px;
.installDialog p
  color: black
.installDialog .dTitle
  font-weight: bold
  text-align: center
  width: 100%
  font-size: 30px;
.installDialog .mainBtn
  width: 100%
